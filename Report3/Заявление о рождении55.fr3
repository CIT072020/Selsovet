;&#10;    strSoob:='—ообщаю следующие сведени€ о себе:';&#13;&#10;    strSoob2:='матери';        &#13;&#10;    if &#60;CurDok.&#34;ONA_CHERN&#34;&#62; &#62; 0 then begin&#13;&#10;      if &#60;CurDok.&#34;ONA_CHERN&#34;&#62; =1 then begin&#13;&#10;        strBrak:='ѕодтверждаю, что в браке не состою.';&#13;&#10;      end else if &#60;CurDok.&#34;ONA_CHERN&#34;&#62; =2 then begin                              &#13;&#10;        strBrak:='ѕодтверждаю, что состою в браке.';&#13;&#10;      end else if &#60;CurDok.&#34;ONA_CHERN&#34;&#62; =3 then begin&#13;&#10;        strBrak:='ѕодтверждаю, что ранее состо€ла в браке.';&#13;&#10;      end;                &#13;&#10;    end else begin&#13;&#10;      strBrak:='ѕодтверждаю, что в браке не состою.';&#13;&#10;    end;              &#13;&#10;  end else begin&#13;&#10;    strSoob:='—ообщаю следующие сведени€ о матери:';          &#13;&#10;    strSoob2:='за€вител€';            &#13;&#10;    if &#60;CurDok.&#34;ONA_CHERN&#34;&#62; &#62; 0 then begin&#13;&#10;      if &#60;CurDok.&#34;ONA_CHERN&#34;&#62; =1 then begin&#13;&#10;        strBrak:='ѕодтверждаю, что мать ребенка в браке не состоит.';&#13;&#10;      end else if &#60;CurDok.&#34;ONA_CHERN&#34;&#62; =2 then begin                              &#13;&#10;        strBrak:='ѕодтверждаю, что мать ребенка состоит в браке.';&#13;&#10;      end else if &#60;CurDok.&#34;ONA_CHERN&#34;&#62; =3 then begin&#13;&#10;        strBrak:='ѕодтверждаю, что мать ребенка ранее состо€ла в браке.';&#13;&#10;      end;                &#13;&#10;    end else begin&#13;&#10;      strBrak:='ѕодтверждаю, что мать ребенка в браке не состоит.';&#13;&#10;    end;              &#13;&#10;  end;            &#13;&#10;&#13;&#10;  //---- “оржественна€ обстановка --------------------------&#13;&#10;  if &#60;CurDok.&#34;TORGESTV&#34;&#62; &#62; 0 then begin                                    &#13;&#10;    strTorgestv:='–егистраци€ рождени€ производитс€ с оказанием дополнительной платной услуги по обеспечению торжественной обстановки';&#13;&#10;    if &#60;CurDok.&#34;TORGESTV&#34;&#62;=1&#13;&#10;      then strTorgestv:=strTorgestv+'  на русском €зыке'                                              &#13;&#10;      else strTorgestv:=strTorgestv+'  на белорусском €зыке';                                                &#13;&#10;  end else begin&#13;&#10;    strTorgestv:='–егистраци€ рождени€ производитс€ без оказани€ дополнительной платной услуги по обеспечению торжественной обстановки';    &#13;&#10;  end;&#13;&#10;      &#13;&#10;  //---- отметка о национальности --------------------------&#13;&#10;  sONA_NATION:='';&#13;&#10;  i:=&#60;CurDok.&#34;ONA_NATION&#34;&#62;;&#13;&#10;  if (i=0) or ((i&#62;=190) and (i&#60;200)) then begin&#13;&#10;    lONA:=false;&#13;&#10;  end else begin                                    &#13;&#10;    lONA:=true;                          &#13;&#10;  end;&#13;&#10;  if lONA then begin&#13;&#10;    mNation.HAlign:=haLeft;                                        &#13;&#10;    sONA:=&#60;CurDok.&#34;VIRT_ONA_OTMETKA&#34;&#62;;&#13;&#10;    sss:=Trim(&#60;CurDok.&#34;OTMETKA&#34;&#62;);&#13;&#10;    if (sONA&#60;&#62;'') then begin                     &#13;&#10;      sONA_NATION:='национальность матери '+sONA;&#13;&#10;    end else begin                                      &#13;&#10;      if sss&#60;&#62;'' then begin&#13;&#10;        i:=Pos('национальность матери',LowerCase(sss));&#13;&#10;        if i&#62;0 then begin&#13;&#10;          sONA_NATION:='национальность '+Copy(sss,i+22,Length(sss));&#13;&#10;          i:=Pos(CRLF,sONA_NATION);&#13;&#10;          j:=Pos('#',sONA_NATION);&#13;&#10;          if (i&#62;0) and ((j=0) or (i&#60;j)) then begin&#13;&#10;            sONA_NATION:=Copy(sONA_NATION,1,i-1);&#13;&#10;          end else if (j&#62;0) and ((i=0) or (j&#60;i)) then begin&#13;&#10;            sONA_NATION:=Copy(sONA_NATION,1,j-1);&#13;&#10;          end;                    &#13;&#10;        end;&#13;&#10;      end;&#13;&#10;    end;&#13;&#1