<TfrxReport Version="4.12" DotMatrixReport="False" IniFile="E:\Projects\Selsovet7\Work\FastReports.ini" OldStyleProgress="True" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="\\NINA\HP LaserJet P1005" PrintOptions.PrintOnSheet="0" ReportOptions.Author="Вадим Чура" ReportOptions.CreateDate="38148.420029456" ReportOptions.Description.Text="" ReportOptions.Name="Запись акта о рождении" ReportOptions.LastChange="43493.681089919" ReportOptions.VersionMajor="6" ReportOptions.VersionMinor="0" ReportOptions.VersionRelease="1" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  strBrak, strTorgestv,strDecl,strDecl1,strOnVozr,strOnaVozr,strDateOn,StrDateOna,strHourHist,strHour,strTime,sOsnZag,sOsn,sOsn2,sss:String;&#13;&#10;  strOtm,strOtmG,strOtmM,strHistAdr,strHistAdrP,strHistAdrM,strHistAdrPM,strHistAdrMM,sSpec:String;&#13;&#10;  strSoob,strSoob2,strHistAdrBel:String;                      &#13;&#10;  strRuk,strSP_J,strSpravka,strONDATER,strONADATER,n31:String;                  &#13;&#10;  nEkz,nLife,nPol,nGorod,nReb,nObraz,nObraz2,i,j,nr:Integer;&#13;&#10;  nP,nM,nPM,nMM:Integer;                                                &#13;&#10;  lZvMat,ll,lll,lONA:boolean;&#13;&#10;  sONA_NATION,sONA:String;                                                                &#13;&#10;&#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Page1, Engine do&#13;&#10;  if (Pos(&#60;CurDok.&#34;ONA_DOK_NOMER&#34;&#62;,&#60;CurDok.&#34;DECL_DOK&#34;&#62;)=0) and&#13;&#10;     (Pos(&#60;CurDok.&#34;ON_DOK_NOMER&#34;&#62;,&#60;CurDok.&#34;DECL_DOK&#34;&#62;)=0) then begin                           &#13;&#10;    lZvMat:=false;&#13;&#10;  end else begin&#13;&#10;    lZvMat:=true;   // заявитель мать                                    &#13;&#10;  end;            &#13;&#10;//[KEY_SVED_MAT]&#13;&#10;//1=в браке не состоит&#13;&#10;//2=состоит в браке&#13;&#10;//3=ранее состояла в браке&#13;&#10;  if lZvMat then begin&#13;&#10;    strSoob:='Сообщаю следующие сведения о себе:';&#13;&#10;    strSoob2:='матери';        &#13;&#10;    if &#60;CurDok.&#34;ONA_CHERN&#34;&#62; &#62; 0 then begin&#13;&#10;      if &#60;CurDok.&#34;ONA_CHERN&#34;&#62; =1 then begin&#13;&#10;        strBrak:='Подтверждаю, что в браке не состою.';&#13;&#10;      end else if &#60;CurDok.&#34;ONA_CHERN&#34;&#62; =2 then begin                              &#13;&#10;        strBrak:='Подтверждаю, что состою в браке.';&#13;&#10;      end else if &#60;CurDok.&#34;ONA_CHERN&#34;&#62; =3 then begin&#13;&#10;        strBrak:='Подтверждаю, что ранее состояла в браке.';&#13;&#10;      end;                &#13;&#10;    end else begin&#13;&#10;      strBrak:='Подтверждаю, что в браке не состою.';&#13;&#10;    end;              &#13;&#10;  end else begin&#13;&#10;    strSoob:='Сообщаю следующие сведения о матери:';          &#13;&#10;    strSoob2:='заявителя';            &#13;&#10;    if &#60;CurDok.&#34;ONA_CHERN&#34;&#62; &#62; 0 then begin&#13;&#10;      if &#60;CurDok.&#34;ONA_CHERN&#34;&#62; =1 then begin&#13;&#10;        strBrak:='Подтверждаю, что мать ребенка в браке не состоит.';&#13;&#10;      end else if &#60;CurDok.&#34;ONA_CHERN&#34;&#62; =2 then begin                              &#13;&#10;        strBrak:='Подтверждаю, что мать ребенка состоит в браке.';&#13;&#10;      end else if &#60;CurDok.&#34;ONA_CHERN&#34;&#62; =3 then begin&#13;&#10;        strBrak:='Подтверждаю, что мать ребенка ранее состояла в браке.';&#13;&#10;      end;                &#13;&#10;    end else begin&#13;&#10;      strBrak:='Подтверждаю, что мать ребенка в браке не состоит.';&#13;&#10;    end;              &#13;&#10;  end;            &#13;&#10;&#13;&#10;  //---- Торжественная обстановка --------------------------&#13;&#10;  if &#60;CurDok.&#34;TORGESTV&#34;&#62; &#62; 0 then begin                                    &#13;&#10;    strTorgestv:='Регистрация рождения производится с оказанием дополнительной платной услуги по обеспечению торжественной обстановки';&#13;&#10;    if &#60;CurDok.&#34;TORGESTV&#34;&#62;=1&#13;&#10;      then strTorgestv:=strTorgestv+'  на русском языке'                                              &#13;&#10;      else strTorgestv:=strTorgestv+'  на белорусском языке';                                                &#13;&#10;  end else begin&#13;&#10;    strTorgestv:='Регистрация рождения производится без оказания дополнительной платной услуги по обеспечению торжественной обстановки';    &#13;&#10;  end;&#13;&#10;      &#13;&#10;  //---- отметка о национальности --------------------------&#13;&#10;  sONA_NATION:='';&#13;&#10;  i:=&#60;CurDok.&#34;ONA_NATION&#34;&#62;;&#13;&#10;  if (i=0) or ((i&#62;=190) and (i&#60;200)) then begin&#13;&#10;    lONA:=false;&#13;&#10;  end else begin                                    &#13;&#10;    lONA:=true;                          &#13;&#10;  end;&#13;&#10;  if lONA then begin&#13;&#10;    mNation.HAlign:=haLeft;                                        &#13;&#10;    sONA:=&#60;CurDok.&#34;VIRT_ONA_OTMETKA&#34;&#62;;&#13;&#10;    sss:=Trim(&#60;CurDok.&#34;OTMETKA&#34;&#62;);&#13;&#10;    if (sONA&#60;&#62;'') then begin                     &#13;&#10;      sONA_NATION:='национальность матери '+sONA;&#13;&#10;    end else begin                                      &#13;&#10;      if sss&#60;&#62;'' then begin&#13;&#10;        i:=Pos('национальность матери',LowerCase(sss));&#13;&#10;        if i&#62;0 then begin&#13;&#10;          sONA_NATION:='национальность '+Copy(sss,i+22,Length(sss));&#13;&#10;          i:=Pos(CRLF,sONA_NATION);&#13;&#10;          j:=Pos('#',sONA_NATION);&#13;&#10;          if (i&#62;0) and ((j=0) or (i&#60;j)) then begin&#13;&#10;            sONA_NATION:=Copy(sONA_NATION,1,i-1);&#13;&#10;          end else if (j&#62;0) and ((i=0) or (j&#60;i)) then begin&#13;&#10;            sONA_NATION:=Copy(sONA_NATION,1,j-1);&#13;&#10;          end;                    &#13;&#10;        end;&#13;&#10;      end;&#13;&#10;    end;&#13;&#10;  end;&#13;&#10;  //--------------------------------------------------------&#13;&#10; &#13;&#10;  if Parameters('ZZ_DOLG')='' then begin&#13;&#10;      Dolg1.Visible := true;&#13;&#10;      Dolg1_1.Visible := true;          &#13;&#10;      Dolg2.Visible := false;&#13;&#10;  end else begin&#13;&#10;      Dolg1.Visible := false;&#13;&#10;      Dolg1_1.Visible := false;          &#13;&#10;      Dolg2.Visible := true;&#13;&#10;  end;&#13;&#10;  if Parameters('ZZ_ORGAN')&#60;&#62;'' then begin&#13;&#10;    Organ2.Height   := 77;&#13;&#10;    Organ2.Width    := 330;&#13;&#10;    Organ2.text     := Parameters('ZZ_ORGAN');&#13;&#10;    Organ1.Visible  := false;&#13;&#10;    Organ3.Visible  := false;&#13;&#10;  end;      &#13;&#10;  begin&#13;&#10;//  sSpec:=RunFuncScript('Zags_Akt_Spec');&#13;&#10;//  if sSpec=''&#13;&#10;//    then mLineSpec.Visible:=false&#13;&#10;//    else mLineSpec.Visible:=true;&#13;&#10;      &#13;&#10;//  ll :=&#60;CurDok.&#34;COPIA&#34;&#62;;&#13;&#10;//  mCopy1.Visible := ll;&#13;&#10;//  mCopy2.Visible := ll;&#13;&#10;//  mCopy3.Visible := ll;&#13;&#10;///  mCopy4.Visible := ll;&#13;&#10;//  Child7.Visible := ll;&#13;&#10;&#13;&#10;  lll :=False;&#13;&#10;  if SubDateYear(&#60;CurDok.&#34;DATEZ&#34;&#62;,&#60;CurDok.&#34;DateR&#34;&#62;)&#62;=1 then lll :=True;&#13;&#10;//  mCopy6.Visible := lll;&#13;&#10;&#13;&#10; if &#60;CurDok.&#34;ON_ONLYGOD&#34;&#62;&#13;&#10;    then StrDateOn := FormatDateTime('YYYY',&#60;CurDok.&#34;ON_DateR&#34;&#62;)+' г.'&#13;&#10;    else StrDateOn := LowerCase(DatePropis(&#60;CurDok.&#34;ON_DateR&#34;&#62;,2));&#13;&#10; if &#60;CurDok.&#34;ONA_ONLYGOD&#34;&#62;&#13;&#10;    then StrDateOna := FormatDateTime('YYYY',&#60;CurDok.&#34;ONA_DateR&#34;&#62;)+' г.'&#13;&#10;    else StrDateOna := LowerCase(DatePropis(&#60;CurDok.&#34;ONA_DateR&#34;&#62;,2));&#13;&#10;&#13;&#10;  if FieldDataSetIsNull('fmZapisRogd.Dokument','TimeR') then begin&#13;&#10;    strHourHist := '';&#13;&#10;    strHour := '';&#13;&#10;    strTime:='';&#13;&#10;  end else begin&#13;&#10;    strTime:='время рождения:';&#13;&#10;    strHourHist := Hist('TimeR');&#13;&#10;    strHour := FormatDateTime('HH час MM мин ', &#60;CurDok.&#34;TimeR&#34;&#62;);&#13;&#10;  end;&#13;&#10;&#13;&#10;  if &#60;CurDok.&#34;First_Ekz&#34;&#62;  then nEkz:=1   else nEkz:=2;&#13;&#10;  if &#60;CurDok.&#34;Givorogd&#34;&#62;   then nLife:=1  else nLife:=2;&#13;&#10;  nReb := &#60;CurDok.&#34;TypeRod&#34;&#62;;&#13;&#10;  if &#60;CurDok.&#34;Pol&#34;&#62;='М'    then nPol:=2   else nPol:=1;&#13;&#10;    nObraz :=&#60;CurDok.&#34;ON_OBRAZ&#34;&#62;;&#13;&#10;  nObraz2:=&#60;CurDok.&#34;ONA_OBRAZ&#34;&#62;;&#13;&#10;  sss:=&#60;CurDok.&#34;SVED_AKT&#34;&#62;;          &#13;&#10;    &#13;&#10;  if GetAddProp('NEWSVED') and (sss&#60;&#62;'') then begin&#13;&#10;    &#13;&#10;    IF &#60;CurDok.&#34;SVED_NOT_SVID&#34;&#62; then begin // свидетельство не представлено&#13;&#10;      sOsnZag:= '';&#13;&#10;      sOsn:=&#60;CurDok.&#34;SVED_SVID&#34;&#62;+CRLF;&#13;&#10;      sOsn2:='запись акта о заключении брака № '+sss+' от '+DatePropis(&#60;CurDok.&#34;SVED_DATEB&#34;&#62;,3);&#13;&#10;      sOsn:=sOsn+sOsn2;&#13;&#10;    end else begin                                    &#13;&#10;      sOsnZag:= IIF(&#60;CurDok.&#34;SVED&#34;&#62;='1','свидетельство о заключении брака','заявление матери');&#13;&#10;      sOsn:=&#60;CurDok.&#34;SVED_SVID&#34;&#62;+CRLF;&#13;&#10;      sOsn2:='запись акта № '+sss+' от '+DatePropis(&#60;CurDok.&#34;SVED_DATEB&#34;&#62;,3);&#13;&#10;      sOsn:=sOsn+sOsn2;&#13;&#10;    end;                &#13;&#10;  end else begin&#13;&#10;    sOsnZag:= IIF(&#60;CurDok.&#34;SVED&#34;&#62;='1',' ','заявление матери');&#13;&#10;    sOsn := &#60;CurDok.&#34;SVED_SVID&#34;&#62;;&#13;&#10;    sss:='';&#13;&#10;    if not DATEISEMPTY(&#60;CurDok.&#34;SVED_DATEB&#34;&#62;) then begin&#13;&#10;      if Pos( FormatDateTime('dd.mm.yyyy', &#60;CurDok.&#34;SVED_DATEB&#34;&#62;), sOsn)=0 then begin&#13;&#10;        sss:= ' '+FormatDateTime('dd.mm.yyyy', &#60;CurDok.&#34;SVED_DATEB&#34;&#62;);&#13;&#10;      end;&#13;&#10;    end;&#13;&#10;   sOsn:=sOsn+sss;&#13;&#10;  end;&#13;&#10;&#13;&#10;  strDecl:=&#60;CurDok.&#34;DECL&#34;&#62;;&#13;&#10;  strDecl1:=&#60;CurDok.&#34;DECL_ADRES&#34;&#62;;      &#13;&#10;//  if &#60;CurDok.&#34;DECL_ADRES&#34;&#62; &#60;&#62;'' then strDecl:=strDecl+', '+&#60;CurDok.&#34;DECL_ADRES&#34;&#62;;&#13;&#10;//  if &#60;CurDok.&#34;DECL_DOK&#34;&#62; &#60;&#62;'' then strDecl:=strDecl+', '+&#60;CurDok.&#34;DECL_DOK&#34;&#62;;&#13;&#10;//  if &#60;CurDok.&#34;TWO_DECL&#34;&#62; &#60;&#62;'' then strDecl:=strDecl+CRLF+&#60;CurDok.&#34;TWO_DECL&#34;&#62;;&#13;&#10;        &#13;&#10;  strHistAdr:=GetAdresAkt('', 'GOSUD,FName;OBL,B_OBL;RAION;GOROD,B_GOROD;',11);&#13;&#10;  if strHistAdr&#60;&#62;'' then begin&#13;&#10;    nR:=1;&#13;&#10;    strHistAdr:=strHistAdr+CRLF;&#13;&#10;  end else begin&#13;&#10;    nR:=0;&#13;&#10;  end;&#13;&#10;  &#13;&#10;  strHistAdrBel:=GetAdresAkt('', '&#60;B&#62;GOSUD,FName_B;OBL_B,B_OBL;RAION_B;GOROD_B,B_GOROD;',11);&#13;&#10;  if strHistAdrBel&#60;&#62;'' &#13;&#10;    then strHistAdrBel:=strHistAdrBel+CRLF;&#13;&#10;  &#13;&#10;  // место жительства&#13;&#10;  strHistAdrP:=GetAdresAkt('', 'ON_GOSUD,FName;ON_OBL,ON_B_OBL;ON_RAION;ON_GOROD;ON_GOROD_R',11);&#13;&#10;  strHistAdrM:=GetAdresAkt('', 'ONA_GOSUD,FName;ONA_OBL,ONA_B_OBL;ONA_RAION;ONA_GOROD;ONA_GOROD_R',11);&#13;&#10;  // место рождения                                        &#13;&#10;  strHistAdrPM:=GetAdresAkt('', 'ON_M_GOSUD,FName;ON_M_OBL,ON_M_B_OBL;ON_M_RAION;ON_M_GOROD;',11);&#13;&#10;  strHistAdrMM:=GetAdresAkt('', 'ONA_M_GOSUD,FName;ONA_M_OBL,ONA_M_B_OBL;ONA_M_RAION;ONA_M_GOROD;',11);&#13;&#10;&#13;&#10;  if  strHistAdrP&#60;&#62;'' then begin&#13;&#10;    nP:=1;&#13;&#10;    strHistAdrP:=strHistAdrP+CRLF;&#13;&#10;  end else begin&#13;&#10;    nP:=0;&#13;&#10;  end;&#13;&#10;  if  strHistAdrM&#60;&#62;'' then begin&#13;&#10;    nM:=1;&#13;&#10;    strHistAdrM:=strHistAdrM+CRLF;&#13;&#10;  end else begin&#13;&#10;    nM:=0;&#13;&#10;  end;&#13;&#10;&#13;&#10;  if  strHistAdrPM&#60;&#62;'' then begin&#13;&#10;    nPM:=1;&#13;&#10;    strHistAdrPM:=strHistAdrPM+CRLF;&#13;&#10;  end else begin&#13;&#10;    nPM:=0;&#13;&#10;  end;&#13;&#10;  if  strHistAdrMM&#60;&#62;'' then begin&#13;&#10;    nMM:=1;&#13;&#10;    strHistAdrMM:=strHistAdrMM+CRLF;&#13;&#10;  end else begin&#13;&#10;    nMM:=0;&#13;&#10;  end;&#13;&#10;&#13;&#10;&#13;&#10;  if parameters('RUK_ZAGS_RN')='' then begin&#13;&#10;     StrRuk:=&#60;CurDok.&#34;RUKOV&#34;&#62;;&#13;&#10;  end else if copy(parameters('RUK_ZAGS_RN'),1,1)='-' then begin&#13;&#10;     StrRuk:='';&#13;&#10;  end else begin&#13;&#10;     StrRuk:=parameters('RUK_ZAGS_RN');&#13;&#10;  end;&#13;&#10;  strSP_J:=GetSP_J(&#60;CurDok.&#34;SP_J_GOD&#34;&#62;,&#60;CurDok.&#34;SP_J&#34;&#62;);                                              &#13;&#10;  if &#60;CurDok.&#34;SPRAV_NOMER&#34;&#62;='' then begin&#13;&#10;     strSpravka:='Справка не выдается.';&#13;&#10;  end else begin&#13;&#10;     strSpravka:='Выдана справка № '+&#60;CurDok.&#34;SPRAV_NOMER&#34;&#62;+' от '+FormatDateTime('dd.mm.yyyy',&#60;CurDok.&#34;SPRAV_DATE&#34;&#62;);&#13;&#10;  end;&#13;&#10;&#13;&#10;  if DateIsEmpty(&#60;CurDok.&#34;ON_DateR&#34;&#62;) then begin&#13;&#10;    if Copy(GetVozrast(&#60;CurDok.&#34;ON_Vozr&#34;&#62;),1,1)='0' then begin&#13;&#10;      strONDATER := CE('');&#13;&#10;    end else begin&#13;&#10;      strONDATER := 'исполнилось '+GetVozrast(&#60;CurDok.&#34;ON_Vozr&#34;&#62;);&#13;&#10;    end;&#13;&#10;    strOnVozr:='';          &#13;&#10;  end else begin&#13;&#10;    if &#60;CurDok.&#34;ON_ONLYGOD&#34;&#62;&#13;&#10;      then strONDateR := FormatDateTime('YYYY',&#60;CurDok.&#34;ON_DateR&#34;&#62;)+' г.'&#13;&#10;      else strONDateR := LowerCase(DatePropis(&#60;CurDok.&#34;ON_DateR&#34;&#62;,4));&#13;&#10;    strOnVozr:=Concat_IF2('исполнилось ',GetVozrastEx(GetCountYear(&#60;CurDok.&#34;DATER&#34;&#62