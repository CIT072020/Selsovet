me:='';&#13;&#10;  end else begin&#13;&#10;    strTime:='врем€ рождени€:';&#13;&#10;    strHourHist := Hist('TimeR');&#13;&#10;    strHour := FormatDateTime('HH час MM мин ', &#60;CurDok.&#34;TimeR&#34;&#62;);&#13;&#10;  end;&#13;&#10;&#13;&#10;  if &#60;CurDok.&#34;First_Ekz&#34;&#62;  then nEkz:=1   else nEkz:=2;&#13;&#10;  if &#60;CurDok.&#34;Givorogd&#34;&#62;   then nLife:=1  else nLife:=2;&#13;&#10;  nReb := &#60;CurDok.&#34;TypeRod&#34;&#62;;&#13;&#10;  if &#60;CurDok.&#34;Pol&#34;&#62;='ћ'    then nPol:=2   else nPol:=1;&#13;&#10;    nObraz :=&#60;CurDok.&#34;ON_OBRAZ&#34;&#62;;&#13;&#10;  nObraz2:=&#60;CurDok.&#34;ONA_OBRAZ&#34;&#62;;&#13;&#10;  sss:=&#60;CurDok.&#34;SVED_AKT&#34;&#62;;          &#13;&#10;    &#13;&#10;  if GetAddProp('NEWSVED') and (sss&#60;&#62;'') then begin&#13;&#10;    &#13;&#10;    IF &#60;CurDok.&#34;SVED_NOT_SVID&#34;&#62; then begin // свидетельство не представлено&#13;&#10;      sOsnZag:= '';&#13;&#10;      sOsn:=&#60;CurDok.&#34;SVED_SVID&#34;&#62;+CRLF;&#13;&#10;      sOsn2:='запись акта о заключении брака є '+sss+' от '+DatePropis(&#60;CurDok.&#34;SVED_DATEB&#34;&#62;,3);&#13;&#10;      sOsn:=sOsn+sOsn2;&#13;&#10;    end else begin                                    &#13;&#10;      sOsnZag:= IIF(&#60;CurDok.&#34;SVED&#34;&#62;='1','свидетельство о заключении брака','за€вление матери');&#13;&#10;      sOsn:=&#60;CurDok.&#34;SVED_SVID&#34;&#62;+CRLF;&#13;&#10;      sOsn2:='запись акта є '+sss+' от '+DatePropis(&#60;CurDok.&#34;SVED_DATEB&#34;&#62;,3);&#13;&#10;      sOsn:=sOsn+sOsn2;&#13;&#10;    end;                &#13;&#10;  end else begin&#13;&#10;    sOsnZag:= IIF(&#60;CurDok.&#34;SVED&#34;&#62;='1',' ','за€вление матери');&#13;&#10;    sOsn := &#60;CurDok.&#34;SVED_SVID&#34;&#62;;&#13;&#10;    sss:='';&#13;&#10;    if not DATEISEMPTY(&#60;CurDok.&#34;SVED_DATEB&#34;&#62;) then begin&#13;&#10;      if Pos( FormatDateTime('dd.mm.yyyy', &#60;CurDok.&#34;SVED_DATEB&#34;&#62;), sOsn)=0 then begin&#13;&#10;        sss:= ' '+Forma