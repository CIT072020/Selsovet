 s:=trim(GetLastValue('ALFAVIT_GOD1','S'));&#13;&#10;  if s='' then edGod1.Text:=Str(YearOf(DATE)-1) else edGod1.Text:=s;&#13;&#10;&#13;&#10;  s:=trim(GetLastValue('ALFAVIT_GOD2','S'));&#13;&#10;  if s='' then edGod2.Text:=Str(YearOf(DATE)-1) else edGod2.Text:=s;&#13;&#10;&#13;&#10;  cbList.Checked:=GetLastValue('ALFAVIT_cbLIST','L');&#13;&#10;  cbAll.Checked:=GetLastValue('ALFAVIT_cbALL','L');&#13;&#10;  cbNameMyOrg.Checked:=GetLastValue('ALFAVIT_cbNAMEMYORG','L');&#13;&#10;  cbNameOrg.Checked:=GetLastValue('ALFAVIT_cbNAMEORG','L');&#13;&#10;  cbKName.Enabled:=cbNameOrg.Checked;                            &#13;&#10;  cbNameMyOrg.Enabled:=cbNameOrg.Checked;                            &#13;&#10;  cbKName.Checked:=GetLastValue('ALFAVIT_cbKNAME','L');&#13;&#10;  cbAllOrgan.Checked:=GetLastValue('ALFAVIT_cbALLORGAN','L');&#13;&#10;  cbAllR.Checked:=GetLastValue('ALFAVIT_cbALLR','L');&#13;&#10;  cbOrder.Checked:=GetLastValue('ALFAVIT_cbORDER','L');&#13;&#10;  n:=GetLastValue('ALFAVIT_cbInt','N');&#13;&#10;  if (n&#62;-1) and (n&#60;3) then cbInt.ItemIndex:=n;                                                                                             &#13;&#10;      &#13;&#10;  edOrgan.KeyValue:=nIDZags;&#13;&#10;  edOrgan.Enabled:=not cbAllOrgan.Checked;&#13;&#10;          &#13;&#10;  if IdProg()='SELSOVET' then begin&#13;&#10;    cbNameOrg.Visible:=false;&#13;&#10;  end else begin&#13;&#10;    cbNameOrg.Visible:=true;&#13;&#10;  end;&#13;&#10;//  par1 := FormatDateTime('yyyy-mm-dd',DATE);&#13;&#10;//  messageBox(par1,'');&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;//-------------------------------------------------------------------                                                                                                                                              &#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Button1, Engine do&#13;&#10;  begin&#13;&#10;&#13;&#10;  if cbList.Checked&#13;&#10;    then GroupHeader1.Condition:='UPPERCASE(COPY(&#60;Query.&#34;Familia&#34;&#62;, 1, 1))'&#13;&#10;    else GroupHeader1.Condition:='';&#13;&#10;  &#13;&#10;  strTypeName:='';&#13;&#10;  if cbKName.Checked then strTypeName:='К';&#13;&#10;  &#13;&#10;  strLast :='';&#13;&#10;  sYear1 := edGod1.text;&#13;&#10;  sYear2 := edGod2.text;&#13;&#10;  if sYear1=sYear2 then begin&#13;&#10;    strYears := 'за ' + sYear2 + ' год';&#13;&#10;  end else begin&#13;&#10;    strYears := 'за ' + sYear1 + ' - ' + sYear2 + ' годы';&#13;&#10;  end;&#13;&#10;  nTypeAkt:=StrToInt(edType.KeyIndex);&#13;&#10;  Query.SQL.text:=LoadSQL('_АлфавиткаАКТ|{'+edType.KeyIndex+'}');                      &#13;&#10;  if Query.SQL.text='' then begin&#13;&#10;    ShowMessage('Ошибка загрузки запроса.');&#13;&#10;    exit;                &#13;&#10;  end;&#13;&#10;    &#13;&#10;  strType:=ValueKey('KEY_TYPEZAGS_FULL',nTypeAkt);&#13;&#10;  MemoNZ.Visible := cbNameOrg.Checked;&#13;&#10;  MemoVZ.Visible := cbNameOrg.Checked;&#13;&#10;  nMax:=6;  // см. объявление переменной arrLine                                                                                                &#13;&#10;  arrLineF[1]:=Line1;                                        &#13;&#10;  arrLineF[2]:=Line2;                                        &#13;&#10;  arrLineF[3]:=Line3;                                        &#13;&#10;  arrLineF[4]:=Line4;                                        &#13;&#10;  arrLineF[5]:=Line5;                                        &#13;&#10;  arrLineF[6]:=Line6;&#13;&#10;    &#13;&#10;  arrLineG[1]:=Line7;&#13;&#10;  arrLineG[2]:=Line8;                                        &#13;&#10;  arrLineG[3]:=Line9;                                        &#13;&#10;  arrLineG[4]:=Line10;                                        &#13;&#10;  arrLineG[5]:=Line11;                                        &#13;&#10;  arrLineG[6]:=Line12;                                        &#13;&#10;  n := 100;&#13;&#10;  m:=60;&#13;&#10;  // 706                 &#13;&#10