;&#10;    s1:='id_zags='+VarToStr(edOrgan.KeyValue);&#13;&#10;    if nIDZags&#60;&#62;edOrgan.KeyValue&#13;&#10;      then strAdd:='('+edOrgan.Text+')'+CRLF;&#13;&#10;  end;&#13;&#10;&#13;&#10;  if edPropAkt.Enabled then begin                                          &#13;&#10;    if (edPropAkt.ItemIndex&#62;-1) then begin&#13;&#10;      if edPropAkt.KeyIndex='2' then begin&#13;&#10;        strAdd:=strAdd+'(первичные записи)';&#13;&#10;        s2:='(vosstan is null or vosstan=false)';&#13;&#10;      end;&#13;&#10;      if edPropAkt.KeyIndex='3' then begin&#13;&#10;        strAdd:=strAdd+'(восстановленные записи)';&#13;&#10;        s2:='vosstan=true';&#13;&#10;      end;&#13;&#10;    end;&#13;&#10;  end;&#13;&#10;    &#13;&#10;  if s1='' then begin&#13;&#10;    if s2&#60;&#62;'' then s1:=s2;&#13;&#10;  end else begin&#13;&#10;    if s2&#60;&#62;'' then s1:='('+s1+' and '+s2+')';&#13;&#10;  end;&#13;&#10;  if s1&#60;&#62;'' then s1:=' and '+s1;&#13;&#10;&#13;&#10;  Query.SQL.text := Replace(Query.SQL.Text, '&#38;add_where&#38;', s1);&#13;&#10;  Query.SQL.text := Replace(Query.SQL.Text, '&#38;Year1&#38;', sYear1);&#13;&#10;  Query.SQL.text := Replace(Query.SQL.Text, '&#38;Year2&#38;', sYear2);&#13;&#10;    &#13;&#10;  if cbOrder.Checked&#13;&#10;    then Query.SQL.text := Replace(Query.SQL.text,'&#38;ORDER_BY&#38;','Substring(Familia,1,1),DateZ,Nomer')&#13;&#10;    else Query.SQL.text := Replace(Query.SQL.text,'&#38;ORDER_BY&#38;','Familia,Name');&#13;&#10;&#13;&#10;  if cbAll.Checked then begin&#13;&#10;    Query.SQL.text := Replace(Query.SQL.text,'&#38;print_all&#38;','');  &#13;&#10;  end else begin                                  &#13;&#10;    Query.SQL.text := Replace(Query.SQL.text,'&#38;print_all&#38;','and 1=2');  &#13;&#10;  end;            &#13;&#10;&#13;&#10;  SetLastValue('ALFAVIT_ORDER_DATEZ',cbOrder.Checked,'L');&#13;&#10;  if nObj=0&#13;&#10;    then SetLastValue('ALFAVIT_TYPE_AKT2',edType.KeyIndex,'S');&#13;&#10;  SetLastValue('ALFAVIT_GOD1',edGod1.Text,'S');&#13;&#10;  SetLastValue('ALFAVIT_GOD2',edGod2.Text,'S');&#13;&#10;  SetLastValue('ALFAVIT_cbLIST',cbList.Checked,'L');&#13;&#10;  SetLastValue('ALFAVIT_cbALL',cbAll.Checked,'L');&#13;&#10;  SetLastValue('ALFAVIT_cbNAMEORG',cbNameOrg.Checked,'L');&#13;&#10;  SetLastValue('ALFAVIT_cbKNAME',cbKName.Checked,'L');&#13;&#10;  SetLastValue('ALFAVIT_cbALLORGAN',cbAllOrgan.Checked,'L');&#13;&#10;  SetLastValue('ALFAVIT_cbALLR',cbAllR.Checked,'L');&#13;&#10;  SetLastValue('ALFAVIT_cbORDER',cbOrder.Checked,'L');&#13;&#10;  SetLastValue('ALFAVIT_cbNAMEMYORG',cbNameMyOrg.Checked,'L');&#13;&#10;  SetLastValue('ALFAVIT_cbInt',cbInt.ItemIndex,'N');&#13;&#10;    &#13;&#10;//  ShowMessage(Query.SQL.text);&#13;&#10;  MemoWrite('report.sql',Query.SQL.Text);                        &#13;&#10;      &#13;&#10;//  sYear1  := edGod.KeyItems[edGod.ItemIndex];&#13;&#10;//  sMonth := edMonth.KeyItems[edMonth.ItemIndex];&#13;&#10;//  strNameMonth := UpperCase(edMonth.Items[edMonth.ItemIndex]);&#13;&#10;//  strNameDate := LowerCase(strNameMonth) + ' ' + sYear;&#13;&#10;  end;          &#13;&#10;end;&#13;&#10;//-------------------------------------------------------------------                                                                                                                                              &#13;&#10;  procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if Engine.FreeSpace &#62; MasterData1.Height-5 then begin&#13;&#10;    if cbOrder.Checked&#13;&#10;      then strLast := Copy(&#60;Query.&#34;Familia&#34;&#62;,1,1)&#13;&#10;      else strLast := Copy(&#60;Query.&#34;Familia&#34;&#62;,1,2);&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;//-------------------------------------------------------------------                                                                                                                                              &#13;&#10;procedure PageHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;