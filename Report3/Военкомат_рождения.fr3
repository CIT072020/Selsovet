    <TfrxADSQuery Name="Query1" UserName="Query1" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT sved, Upper(iif(sved=3,Ona_Familia,On_Familia)) FAM_SORT,&#13;&#10;       iif(sved=3, IFNULL(ONA_VOENKOM,&#38;vvv&#38;), IFNULL(ON_VOENKOM,&#38;vvv&#38;)) VOENKOM,&#13;&#10;       Nomer, DayOfMonth(datez) as dayz, datez, Trim(Familia)+' '+Trim(IFNULL(Name,''))+' '+Trim(IFNULL(Otch,'')) as FIO, DateR,&#13;&#10;       iif(sved=3,'',trim(On_Familia)+' '+trim(IFNULL(On_Name,''))+' '+trim(IFNULL(On_Otch,''))) as ON_FIO, On_DateR,&#13;&#10;       On_Gosud, On_B_Obl,On_Obl, On_Raion, On_B_Rn, On_Gorod, ON_B_GOROD, On_Gorod_R, On_Dom, On_Korp, On_Kv, ON_Dokument,&#13;&#10;       trim(Ona_Familia)+' '+trim(IFNULL(Ona_Name,''))+' '+trim(IFNULL(Ona_Otch,'')) as ONA_FIO, Ona_DateR,&#13;&#10;       Ona_Gosud, Ona_B_Obl, Ona_Obl, Ona_Raion, Ona_B_Rn, Ona_Gorod, ONA_B_GOROD, Ona_Gorod_R, Ona_Dom, Ona_Korp, Ona_Kv,&#13;&#10;       ID_ZAGS, SVED&#13;&#10;  FROM АктыРождений&#13;&#10;  WHERE datez&#62;=:strDate1 and datez&#60;=:strDate2 and  (vosstan is null or vosstan=false) and  &#13;&#10;        (sved=1 or sved=:nnSved) and ( (on_grag=112 and GetVozrast(datez,on_dater)&#60;=:nnVozr) or sved=:nnSved)&#13;&#10;   UNION ALL   /*---------*/&#13;&#10;SELECT sved, Ona_Familia FAM_SORT,&#13;&#10;       IFNULL(ONA_VOENKOM,1) VOENKOM,&#13;&#10;       Nomer, DayOfMonth(datez) as dayz, datez, Trim(Familia)+' '+Trim(IFNULL(Name,''))+' '+Trim(IFNULL(Otch,'')) as FIO, DateR,&#13;&#10;       trim(On_Familia)+' '+trim(IFNULL(On_Name,''))+' '+trim(IFNULL(On_Otch,'')) as ON_FIO, On_DateR,&#13;&#10;       On_Gosud, On_B_Obl,On_Obl, On_Raion, On_B_Rn, On_Gorod, ON_B_GOROD, On_Gorod_R, On_Dom, On_Korp, On_Kv, ON_Dokument,&#13;&#10;       trim(Ona_Familia)+' '+trim(IFNULL(Ona_Name,''))+' '+trim(IFNULL(Ona_Otch,'')) as ONA_FIO, Ona_DateR,&#13;&#10;       Ona_Gosud, Ona_B_Obl, Ona_Obl, Ona_Raion, Ona_B_Rn, Ona_Gorod, ONA_B_GOROD, Ona_Gorod_R, Ona_Dom, Ona_Korp, Ona_Kv,&#13;&#10;       ID_ZAGS, SVED&#13;&#10;  FROM АктыРождений&#13;&#10;  WHERE datez&#62;=:strDate1 and datez&#60;=:strDate2 and  (vosstan is null or vosstan=false) and sved&#60;&#62;3 and&#13;&#10;        IFNULL(ONA_VOENKOM,0)&#62;0 and IFNULL(ONA_VOENKOM,0)&#60;&#62;IFNULL(ON_VOENKOM,0) and 1=0&#13;&#10;        &#13;&#10;  ORDER BY &#38;ORDER_BY&#38;" SourceTableType="ttAdsADT" CharType="ANSI" RequestLive="False" PropData=""/>
