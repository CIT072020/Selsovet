<TfrxReport Version="4.12" DotMatrixReport="False" IniFile="E:\Projects\Selsovet7\Work\FastReports.ini" OldStyleProgress="True" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.Author="Вадим Чура" ReportOptions.CreateDate="38251.4373884143" ReportOptions.Description.Text="" ReportOptions.LastChange="43742.6980940625" ReportOptions.VersionMajor="1" ReportOptions.VersionMinor="1" ReportOptions.VersionRelease="1" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  s,sDate1,sDate2,strNameDate,strOnAdres,strOn,strOnaAdres,strOna,strV1:String;                                 &#13;&#10;  nVozr,nSved,n:Integer;&#13;&#10;  l,lVoenkom:Boolean;&#13;&#10;    &#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with DialogPage1, Engine do&#13;&#10;  begin&#13;&#10;  nVozr:=50;     // органичение на возраст отца&#13;&#10;//  if GetLastValue('VOZRAST_OTEC','S')=''&#13;&#10;//    then edVozr.TextChecked:=false&#13;&#10;//    else cbOdin.Checked:=true;&#13;&#10;&#13;&#10;  if GetLastValue('ADD_MATER_ODIN','N')=-1&#13;&#10;    then cbOdin.Checked:=false&#13;&#10;    else cbOdin.Checked:=true;&#13;&#10;&#13;&#10;//  nV1.Text := GetLastValue('MIN_VOZRAST_VOENKOM_R','S');&#13;&#10;//  if nV1.Text='' then nV1.Text:='16';&#13;&#10;  nV2.Text := GetLastValue('MAX_VOZRAST_VOENKOM_R','S');&#13;&#10;  if nV2.Text='' then nV2.Text:='50';&#13;&#10;&#13;&#10;  DateEdit1.Date:=Date-8;&#13;&#10;  DateEdit2.Date:=Date-1;&#13;&#10;  &#13;&#10;  if Trim(GetLastValue('RAZREZ_VOENKOM','S'))='1'&#13;&#10;    then cbVoenkom.Checked:=true&#13;&#10;    else cbVoenkom.Checked:=false;&#13;&#10;&#13;&#10;  if IDProg()='ZAGS' then begin&#13;&#10;    cbAll.Visible:=true;&#13;&#10;    if GetLastValue('ALL_ZAGS','N')=-1&#13;&#10;      then cbAll.Checked:=false&#13;&#10;      else cbAll.Checked:=true;&#13;&#10;  end else begin&#13;&#10;    cbAll.Visible:=false;&#13;&#10;  end;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Button1, Engine do&#13;&#10;  begin&#13;&#10;//  strDate1 := edGod.KeyItems;&#13;&#10;//  sYear  := edGod.KeyItems[edGod.ItemIndex];&#13;&#10;//  sMonth := edMonth.KeyItems[edMonth.ItemIndex];&#13;&#10;//    sDate1 := DateEdit1.Date;   не работает на компьютерах где некоррктно уст. региональная дата&#13;&#10;//    sDate2 := DateEdit2.Date;&#13;&#10;  sDate1 := FormatDateTime('yyyy-mm-dd',DateEdit1.Date);&#13;&#10;  sDate2 := FormatDateTime('yyyy-mm-dd',DateEdit2.Date);&#13;&#10;  lVoenkom:=cbVoenkom.Checked;        &#13;&#10;  strNameDate := 'с '+FormatDateTime('dd.mm.yyyy',DateEdit1.Date)+'г. по '+FormatDateTime('dd.mm.yyyy',DateEdit2.Date)+'г. ';&#13;&#10;  if cbAllMens.Checked then begin&#13;&#10;    strV1:='Отчет о cоставленых записях актов о рождении за период'+CRLF+strNameDate;    &#13;&#10;    strNameDate:='';                  &#13;&#10;    lVoenkom:=false;        &#13;&#10;    Query1.SQL.text := Replace(Query1.SQL.text,'&#38;vvv&#38;','0');&#13;&#10;    n:=Pos('WHERE',Query1.SQL.text);&#13;&#10;    s:=Copy(Query1.SQL.text,1,n-1)+CRLF;&#13;&#10;    s:=s+'WHERE datez&#62;='+DateToSQL(DateEdit1.Date)+' and datez&#60;='+DateToSQL(DateEdit2.Date)+' and  (vosstan is null or vosstan=false)'+CRLF;&#13;&#10;    s:=s+'ORDER BY Fam_sort'+;&#13;&#10;    Query1.SQL.text:=s;&#13;&#10;  end else begin                                  &#13;&#10;    strNameDate := 'с '+FormatDateTime('dd.mm.yyyy',DateEdit1.Date)+'г. по '+FormatDateTime('dd.mm.yyyy',DateEdit2.Date)+'г. '+&#13;&#10;                   'cоставлены записи актов о рождении: ';&#13;&#10;    nVozr:=StrToInt(nV2.text);&#13;&#10;    Query1.SQL.text := Replace(Query1.SQL.text,'&#38;vvv&#38;',Parameters('ID_VOENKOM'));&#13;&#10;    if lVoenkom then begin&#13;&#10;      Query1.SQL.text := Replace(Query1.SQL.text,'&#38;ORDER_BY&#38;','Voenkom,Fam_sort');  &#13;&#10;      Query1.SQL.text := Replace(Query1.SQL.text,'and 1=0','and 1=1');  &#13;&#10;    end else begin                                      &#13;&#10;      Query1.SQL.text := Replace(Query1.SQL.text,'&#38;ORDER_BY&#38;','Fam_sort');&#13;&#10;    end;&#13;&#10;    Query1.Filter:='';&#13;&#10;    if cbOdin.Checked&#13;&#10;      then nSved:=3&#13;&#10;      else nSved:=-1;&#13;&#10;    SetLastValue('ADD_MATER_ODIN',nSved, 'N');&#13;&#10;    SetLastValue('MAX_VOZRAST_VOENKOM_R',nV2.Text,'S');&#13;&#10;    if lVoenkom&#13;&#10;      then SetLastValue('RAZREZ_VOENKOM','1','S')&#13;&#10;      else SetLastValue('RAZREZ_VOENKOM','0','S');&#13;&#10;    if cbAll.Visible then begin&#13;&#10;      if cbAll.Checked&#13;&#10;        then n:=1&#13;&#10;        else n:=-1;&#13;&#10;      SetLastValue('ALL_ZAGS',n, 'N');&#13;&#10;      if not cbAll.Checked then begin&#13;&#10;        if Query1.Filter&#60;&#62;'' then Query1.Filter:=Query1.Filter+' and ';&#13;&#10;        Query1.Filter:=Query1.Filter+'id_zags='+Parameters('ID');&#13;&#10;        Query1.Filtered:=true;&#13;&#10;      end;&#13;&#10;    end;&#13;&#10;  end;        &#13;&#10;    &#13;&#10; memowrite('report.sql',Query1.SQL.text);    &#13;&#10;//  MessageBox(Query1.SQL);&#13;&#10;//  strRogd := Query1.FieldGet('kolvo','S');&#13;&#10;//  Query1.Next;&#13;&#10;//  strBrak:= Query1.FieldGet('kolvo','S');&#13;&#10;//  Query1.First;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with MasterData2, Engine do&#13;&#10;  begin&#13;&#10;&#13;&#10;  if (&#60;Query1.&#34;Sved&#34;&#62;=3) then begin&#13;&#10;    strOn:='по заялению матери ';&#13;&#10;    strOnAdres:='';&#13;&#10;  end else begin&#13;&#10;    strOn:=FirstUpper(&#60;Query1.&#34;ON_FIO&#34;&#62;);&#13;&#10;    strOnAdres  := GetAdresAkt2('Query1', 'ON_GOSUD,FName;ON_OBL,ON_B_OBL;ON_RAION,ON_B_RN,0;ON_GOROD,ON_B_GOROD;ON_GOROD_R,ON_*',1);&#13;&#10;    if strOn&#60;&#62;'' then begin&#13;&#10;      strOn:=strOn+' , '+DatePropis(&#60;Query1.&#34;ON_DATER&#34;&#62;,3)+'г.';&#13;&#10;    end;&#13;&#10;  end;&#13;&#10;&#13;&#10;  strOna:=FirstUpper(&#60;Query1.&#34;ONA_FIO&#34;&#62;);&#13;&#10;  strOnaAdres:='';&#13;&#10;  if strOna&#60;&#62;'' then begin&#13;&#10;    strOna:=strOna+' , '+DatePropis(&#60;Query1.&#34;ONA_DATER&#34;&#62;,3)+'г.';&#13;&#10;    strOnaAdres := GetAdresAkt2('Query1', 'ONA_GOSUD,FName;ONA_OBL,ONA_B_OBL;ONA_RAION,ONA_B_RN,0;ONA_GOROD,ONA_B_GOROD;ONA_GOROD_R,ONA_*',1);&#13;&#10;  end;&#13;&#10;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with GroupHeader1, Engine do begin&#13;&#10;    if not cbAllMens.Checked then begin                     &#13;&#10;      if lVoenkom&#13;&#10;        then s:=&#60;Query1.&#34;VOENKOM&#34;&#62;&#13;&#10;        else s:=Parameters('ID_VOENKOM');&#13;&#10;    &#13;&#10;      if Parameters('VOENKOM_ADRES')='1'  then strV1:=ValuesSpr('sprVoenkom-&#62;ID',s,'Name;Adres;Telefon',CRLF)&#13;&#10;                                          else strV1:=ValueSpr('sprVoenkom-&#62;ID',s,'NAME');                                       &#13;&#10;      if strV1=''&#13;&#10;        then strV1:=ValueSpr('sprVoenkom-&#62;ID',s,'Name');  &#13;&#10;    end;          &#13;&#10;  end;  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;    with Page2, Engine do&#13;&#10;  begin&#13;&#10;  if lVoenkom then begin&#13;&#10;    SetLastValue('RAZREZ_VOENKOM','1', 'S');&#13;&#10;    GroupHeader1.Condition:='&#60;Query1.&#34;VOENKOM&#34;&#62;';&#13;&#10;///    MasterHeader1.RepeatHeader:=true;&#13;&#10;  end else begin&#13;&#10;    SetLastValue('RAZREZ_VOENKOM','0', 'S');&#13;&#10;    GroupHeader1.Condition:='1';&#13;&#10;///    MasterHeader1.RepeatHeader:=true;&#13;&#10;  end;&#13;&#10;&#13;&#10;//  MessageBox(Parameters(' main_select'),'');&#13;&#10;  end&#13;&#10;  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure cbAllMensOnClick