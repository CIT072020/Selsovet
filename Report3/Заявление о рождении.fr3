                  &#13;&#10;        end;&#13;&#10;      end;&#13;&#10;    end;&#13;&#10;  end;&#13;&#10;  if lONA then begin                                                 &#13;&#10;    if (sONA&#60;&#62;'') then begin                     &#13;&#10;      sONA_NATION:='национальность матери '+sONA;&#13;&#10;    end else begin                                      &#13;&#10;      if sss&#60;&#62;'' then begin&#13;&#10;        i:=Pos('национальность матери',LowerCase(sss));&#13;&#10;        if i&#62;0 then begin&#13;&#10;          sONA_NATION:='национальность '+Copy(sss,i+22,Length(sss));&#13;&#10;          i:=Pos(CRLF,sONA_NATION);&#13;&#10;          j:=Pos('#',sONA_NATION);&#13;&#10;          if (i&#62;0) and ((j=0) or (i&#60;j)) then begin&#13;&#10;            sONA_NATION:=Copy(sONA_NATION,1,i-1);&#13;&#10;          end else if (j&#62;0) and ((i=0) or (j&#60;i)) then begin&#13;&#10;            sONA_NATION:=Copy(sONA_NATION,1,j-1);&#13;&#10;          end;                    &#13;&#10;        end;&#13;&#10;      end;&#13;&#10;    end;&#13;&#10;  end;&#13;&#10;  //--------------------------------------------------------&#13;&#10;    &#13;&#10;  if Parameters('ZZ_DOLG')='' then begin&#13;&#10;      Dolg1.Visible := true;&#13;&#10;      Dolg1_1.Visible := true;          &#13;&#10;      Dolg2.Visible := false;&#13;&#10;  end else begin&#13;&#10;      Dolg1.Visible := false;&#13;&#10;      Dolg1_1.Visible := false;          &#13;&#10;      Dolg2.Visible := true;&#13;&#10;  end;&#13;&#10;  if Parameters('ZZ_ORGAN')&#60;&#62;'' then begin&#13;&#10;    Organ2.Height   := 77;&#13;&#10;    Organ2.Width    := 330;&#13;&#10;    Organ2.text     := Parameters('ZZ_ORGAN');&#13;&#10;    Organ1.Visible  := false;&#13;&#10;    Organ3.Visible  := false;&#13;&#10;  end;      &#13;&#10;  begin&#13;&#10;//  sSpec:=RunFuncScript('Zags_Akt_Spec');&#13;&#10;//  if sSpec=''&#13;&#10;//    then mLineSpec.Visible:=false&#13;&#10;//    else mLineSpec.Visible:=true;&#13;&#10;      &#13;&#10;//  ll