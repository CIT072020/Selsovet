ue('ADD_SVID','C')='1' then cbSvid.Checked:=true  else cbSvid.Checked:=false;&#13;&#10;    &#13;&#10;  edSdan.KeyIndex:='2';&#13;&#10;  cbBomg.ItemIndex:=2;   // отдельны списком                                           &#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Page2, Engine do begin&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  setRepeatReport(true);                                            &#13;&#10;  with Button1, Engine do&#13;&#10;  begin&#13;&#10;{  &#13;&#10;0=да&#13;&#10;1=нет&#13;&#10;2=отдельным списком&#13;&#10;}    &#13;&#10;  //---- бомжы ------------------&#13;&#10;  if cbBomg.ItemIndex=1&#13;&#10;    then lBomg:=false    // исключить бомжей                                               &#13;&#10;    else lBomg:=true;    // (0-да, 2-отдельным списком)                                                                &#13;&#10;  if cbBomg.ItemIndex=2&#13;&#10;    then lBomgOtd:=true      &#13;&#10;    else lBomgOtd:=false;                                                                  &#13;&#10;  //----------------------------------------------------&#13;&#10;&#13;&#10;  strRaion := '';&#13;&#10;  s:='';&#13;&#10;&#13;&#10;  if cbOnlyNotRn.Checked then begin // только не мой город&#13;&#10;    if cbRaion.Checked&#13;&#10;      then GroupHeader1.Condition:='&#60;Query1.&#34;RAION_ORDER&#34;&#62;'&#13;&#10;      else GroupHeader1.Condition:='1';  &#13;&#10;  end else begin&#13;&#10;    cbRaion.Checked:=false;                &#13;&#10;    if Trim(RnGorod.Text)='' then begin&#13;&#10;      GroupHeader1.Condition:='&#60;Query1.&#34;RAION_ORDER&#34;&#62;';&#13;&#10;    end else begin&#13;&#10;      GroupHeader1.Condition:='1';&#13;&#10;    end;&#13;&#10;  end;&#13;&#10;    &#13;&#10;  if cbNomer.Checked or cbSvid.Checked then begin          &#13;&#10;    mPrim.Text:='';          &#13;&#10;    if cbNomer.Checked then&#13;&#10;      mPrim.Text:='запись акта'+#13;          &#13;&#10;    if cbSvid.Checked then &#13;&#10;      mPrim.Text:='свид-во';          &#13;&#10;  end;                &#13;&#10;      &#13;&#10;  if cbOnlyNotRn.Checked then begin // только не мой город&#13;&#10;    if cbRaion.Checked&#13;&#10;      then sXXX:='IIF(soato is null, ''0000000'', IIF(substring(soato,2,1)=''4'',substring(soato,1,7),substring(soato,1,4)+''000''))'&#13;&#10;      else sXXX:='GT_RAION';&#13;&#10;    s:=' and GT_GOROD&#60;&#62;'+QStr(strMyGorod);&#13;&#10;    if (RnGorod.Text&#60;&#62;'') then begin&#13;&#10;      s:=s+' and (GT_RAION='+QStr(RnGorod.Text)+')';  &#13;&#10;      strRaion := RnGorod.Text + ' район';&#13;&#10;    end;&#13;&#10;  end else begin&#13;&#10;    if lBomgOtd &#13;&#10;      then sXXX:='case when bomg=true then '' БОМЖ'' '+#13+&#13;&#10;                      'when gt_gorod='+QStr(strMyGorod)+' and gt_raion&#60;&#62;'''' then UPPER(gt_raion) '+#13+&#13;&#10;                      'when gt_gorod='+QStr(strMyGorod)+' and gt_raion=''''  then '' БЕЗ РАЙОНА'' '+#13+&#13;&#10;                      'else '+QStr('Не '+strMyGorod)+' end '  &#13;&#10;      else sXXX:='case when gt_gorod='+QStr(strMyGorod)+' and gt_raion&#60;&#62;'''' then UPPER(gt_raion) '+#13+&#13;&#10;                      'when gt_gorod='+QStr(strMyGorod)+' and gt_raion=''''  then '' БЕЗ РАЙОНА'' '+#13+&#13;&#10;                      'else '+QStr('Не '+strMyGorod)+' end ';    &#13;&#10;    if (RnGorod.Text&#60;&#62;'') then begin&#13;&#10;      s:=' and GT_GOROD='+QStr(strMyGorod)+' and GT_RAION='+QStr(RnGorod.Text);&#13;&#10;      strRaion := RnGorod.Text + ' район';&#13;&#10;    end;&#13;&#10;  end;&#13;&#10;&#13;&#10;  Query1.SQL.Text := Replace(Query1.SQL.Text,QStr('XXX'), sXXX);&#13;&#10;&#13;&#10;  n:=Pos('ORDER BY', Query1.SQL.Text);&#13;&#10;  if cbSortNomer.Checked then begin                                                                             &#13;&#10;    Query1.SQL.Text := Copy(Query1.SQL.Text,1,n-1)+' ORDER BY RAION_ORDER, NOMER';&#13;&#10;  end else begin                                    &#13;&#10;    Query1.SQL.Text := Copy(Query1.SQL.Text,1,n-1)+' ORDER BY RAION_ORDER, FAMILIA';&#13;&#10;  end;            &#13;&#10;             &#13;&#10;  strSSS:='Направляем паспорта, сданные в связи со смертью граждан';&#13;&#10;  if edSdan.KeyIndex='1' then begin&#13;&#10;    s:=s+' and IS_SDAN_DOK=true';&#13;&#10;    ss:=cbTypeDok.GetFilterString();&#13;&#10;    if ss&#60;&#62;'' then begin&#13;&#10;      s:=s+' and ('+ss+')';&#13;&#10;    end;&#13;&#10;  end else if edSdan.KeyIndex='0' then begin&#13;&#10;    s:=s+' and IS_SDAN_DOK=false';&#13;&#10;    strSSS:='Направляем список граждан не сдавших паспорта';&#13;&#10;  end;&#13;&#10;&#13;&#10;  if not lBomg &#13;&#10;    then s:=s+' and (BOMG=false or BOMG is null)';&#13;&#10;&#13;&#10; // ShowMessage(s+#13+Query1.SQL.Text);&#13;&#10;    &#13;&#10;  if s&#60;&#62;'' then                               &#13;&#10;    Query1.SQL.Text := Replace(Query1.SQL.Text, 'and 1=1', s);&#13;&#10;&#13;&#10;  sYear  := Trim(edGod.Text);&#13;&#10;  if sYear='' then sYear:=Str(YearOf(Date));&#13;&#10;    &#13;&#10;  sMonth := Trim(edMonth.KeyIndex);&#13;&#10;  lErr:=false;&#13;&#10;  if sYear='' then begin&#13;&#10;    ShowMessage(' Не выбран год. ');&#13;&#10;    sYear:='1500';&#13;&#10;    lErr:=true;&#13;&#10;  end;&#13;&#10;  if sMonth='' then begin&#13;&#10;    ShowMessage(' Не выбран месяц. ');&#13;&#10;    sMonth:='1';&#13;&#10;    lErr:=true;&#13;&#10;  end;&#13;&#10;//  sYear  := edGod.KeyItems[edGod.ItemIndex];&#13;&#10;//  sMonth := edMonth.KeyItems[edMonth.ItemIndex];&#13;&#10;//  ' Year(datez)='+cbYear.Items[cbYear.ItemIndex]&#13;&#10;  if not lErr then begin&#13;&#10;    strNameMonth := UpperCase(edMonth.Items[edMonth.ItemIndex]);&#13;&#10;    strNameDate := ' за '+LowerCase(strNameMonth) + ' ' + sYear+' года';&#13;&#10;  end else begin&#13;&#10;    strNameMonth := '';&#13;&#10;    strNameDate := '';&#13;&#10;  end;&#13;&#10;&#13;&#10;//  strDate1 := edGod.KeyItems;&#13;&#10;//  sYear  := edGod.KeyItems[edGod.ItemIndex];&#13;&#10;//  sMonth := edMonth.KeyItems[edMonth.ItemIndex];&#13;&#10;//  ' Year(datez)='+cbYear.Items[cbYear.ItemIndex]&#13;&#10;//  messagebox('',sYear+'  '+sMonth);&#13;&#10;//  strNameMonth := UpperCase(edMonth.Items[edMonth.ItemIndex]);&#13;&#10;//  strNameDate := LowerCase(strNameMonth) + ' ' + sYear;&#13;&#10;&#13;&#10;  if cbAllOrgan.Checked=true then SetLastValue('ALL_ZAGS', '1', 'C') else SetLastValue('ALL_ZAGS', '0', 'C');&#13;&#10;  if cbNomer.Checked=true then SetLastValue('ADD_AZ', '1', 'C')   else SetLastValue('ADD_AZ', '0', 'C');&#13;&#10;  if cbSvid.Checked=true  then SetLastValue('ADD_SVID', '1', 'C') else SetLastValue('ADD_SVID', '0', 'C');&#13;&#10;  if cbSortNomer.Checked=true  then SetLastValue('ORDERBY_NOMER_IN_ROVD','1','C') else SetLastValue('ORDERBY_NOMER_IN_ROVD','0','C');             &#13;&#10;      &#13;&#10;  SetLastValue('LAST_MONTH', edMonth.KeyIndex, 'S');&#13;&#10;    &#13;&#10;  if (IDProg()='ZAGS') and not cbAllOrgan.Checked then begin&#13;&#10;    Query1.Filter:='id_zags='+Parameters('ID');&#13;&#10;    Query1.Filtered:=true;&#13;&#10;  end else begin                                    &#13;&#10;    Query1.Filter:='';&#13;&#10;    Query1.Filtered:=false;&#13;&#10;  end;        &#13;&#10;&#13;&#10;//  showmessage(Query1.SQL.Text);&#13;&#10;&#13;&#10;  end;  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure cbOnlyNotRnOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  cbRaion.Enabled:=cbOnlyNotRn.Checked;                                                        &#13;&#10;  if not cbOnlyNotRn.Checked and cbRaion.Checked&#13;&#10;    then cbRaion.Checked:=false;                                 &#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  strRaion:=Trim(UpperCase(&#60;Query1.&#34;RAION_ORDER&#34;&#62;));&#13;&#10;  if Trim(UpperCase(&#60;Query1.&#34;GOROD&#34;&#62;))=strMyGorod &#13;&#10;    then strAdres := GetAdresAkt2('FRX.Query1', '&#60;.&#62;GOSUD,FName;OBL,B_OBL;RAION,B_RN;;GOROD_R,*',1)&#13;&#10;    else strAdres := GetAdresAkt2('FRX.Query1', '&#60;.&#62;GOSUD,FName;OBL,B_OBL;RAION,B_RN;GOROD,B_GOROD;GOROD_R,*',1);&#13;&#10;      &#13;&#10;  strPrim:='';&#13;&#10;  if cbNomer.Checked then begin&#13;&#10;    s:=DatePropis(&#60;Query1.&#34;DateZ&#34;&#62;,3);&#13;&#10;    s:=Copy(s,1,6)+Copy(s,9,2);                                                              &#13;&#10;    strPrim := Str(&#60;Query1.&#34;Nomer&#34;&#62;) + ' от ' + s+#13;&#13;&#10;  end;&#13;&#10;  if cbSvid.Checked then begin&#13;&#10;    strPrim := strPrim+&#60;Query1.&#34;SVID_SERIA&#34;&#62; + '  ' + &#60;Query1.&#34;SVID_NOMER&#34;&#62;;&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if cbRaion.Checked then begin&#13;&#10;    strGroup:=VarToStr(SeekValueSpr('SprRovd', 'SOATO', &#60;Query1.&#34;SOAT7&#34;&#62;+'000', 'FNAME'));&#13;&#10;    strRaion:='';&#13;&#10;    if strGroup='' then strGroup:=GetNameTerr(&#60;Query1.&#34;SOAT7&#34;&#62;+'000', true, true, false);                                                                                                 &#13;&#10;  end else begin                                  &#13;&#10;    strGroup:=Parameters('ROVD');&#13;&#10;    strRaion:=Trim(UpperCase(&#60;Query1.&#34;RAION_ORDER&#34;&#62;));&#13;&#10;    if (strRaion&#60;&#62;'') and (Pos('РАЙОН',strRaion)=0) and (Pos('БОМЖ',strRaion)=0) and (Pos('НЕ ',strRaion)=0)then begin&#13;&#10;      strRAION:=strRAION+' '+'РАЙОН';                                                                    &#13;&#10;    end;&#13;&#10;  end;              &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" PropData="044C65667403A80203546F7003000108446174617365747301010C2600000020446174615365743D225175657279312220446174