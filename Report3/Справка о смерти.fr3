;&#10;  end;&#13;&#10;  &#13;&#10;  strMesto := GetAdresAkt2('fmZapisSmert.Dokument', 'SM_GOSUD,FName;+SM_OBL,SM_B_OBL;SM_RAION;SM_GOROD,SM_B_GOROD;',3);&#13;&#10;  strNorm1 := '    ';&#13;&#10;//  strBold1 := 'Гр. '+[fmZapisSmert.Dokument.&#34;Familia&#34;]+' '+[fmZapisSmert.Dokument.&#34;Name&#34;]+' '+[fmZapisSmert.Dokument.&#34;Otch&#34;];&#13;&#10;  strBold1:='Гр. '+GetPadegFIO3(&#60;CurDok.&#34;Familia&#34;&#62;,&#60;CurDok.&#34;Name&#34;&#62;,&#60;CurDok.&#34;Otch&#34;&#62;,&#60;CurDok.&#34;POL&#34;&#62;,'И');&#13;&#10;&#13;&#10;  strNorm2:='';&#13;&#10;  if YearOf(NOW)&#62;2015 then begin&#13;&#10;    strNorm2:=', '+getResource('IN',0)+': ';&#13;&#10;    strIN:=Trim(CondIN('LICH_NOMER'));                                                      &#13;&#10;    if strIN=''&#13;&#10;      then strNorm2:=strNorm2+' '+getResource('NONE',0)+','&#13;&#10;      else strNorm2:=strNorm2+strIN+',';&#13;&#10;  end;&#13;&#10;     &#13;&#10;  if &#60;CurDok.&#34;ONLYGOD&#34;&#62;=0 then begin&#13;&#10;    strNorm2 :=strNorm2+' '+IIF(&#60;CurDok.&#34;POL&#34;&#62;='М','умер ','умерла ')+DatePropisEx(&#60;CurDok.&#34;DATES&#34;&#62;,4,&#60;CurDok.&#34;ONLYGOD&#34;&#62;);&#13;&#10;  end else begin&#13;&#10;    strNorm2 :=strNorm2+' '+IIF(&#60;CurDok.&#34;POL&#34;&#62;='М','умер ','умерла ')+': '+DatePropisEx(&#60;CurDok.&#34;DATES&#34;&#62;,4,&#60;CurDok.&#34;ONLYGOD&#34;&#62;);&#13;&#10;  end;&#13;&#10;//  showmessage(strnorm2);                            &#13;&#10;  strNorm2 := strNorm2 + strVozr;&#13;&#10;  strNorm2 := strNorm2 + ', место смерти: '+strMesto+', о чём ';&#13;&#10;  strNorm2 := strNorm2 + SprNameOrg(&#60;CurDok.&#34;ID_ZAGS&#34;&#62;,'','Т')+' '+DatePropis(&#60;CurDok.&#34;DATEZ&#34;&#62;,4)+'';&#13;&#10;  strNorm2 := strNorm2 + ' совершена запись акта о смерти № '+Str(&#60;CurDok.&#34;NOMER&#34;&#62;)+'.';&#13;&#10;//  strNorm2 := strNorm2 + CRLF + '        Справка действительна в течение шести месяцев.';&#13;&#10;&#13;&#10;  end&#13;&#10; end;&#13;&#10;&#13;&#1