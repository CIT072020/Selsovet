айон';&#13;&#10;    end else if cbRaion.Checked then  begin  // делить по районам                                           &#13;&#10;      &#13;&#10;    end;              &#13;&#10;  end;&#13;&#10;  if (IDProg()='ZAGS') and not cbAllOrgan.Checked then begin&#13;&#10;    if s=''&#13;&#10;      then s:=s+' and id_zags='+Parameters('ID');                                                     &#13;&#10;  end;&#13;&#10;  Query1.SQL.text := Replace(Query1.SQL.text, '&#38;RN_GOROD&#38;', s);&#13;&#10;  //===============================================================&#13;&#10;  Query1.SQL.text := Replace(Query1.SQL.text, '&#38;ADD_WHERE&#38;', sAdd);&#13;&#10;      &#13;&#10;  strSSS:='Направляем паспорта, сданные в связи со смертью граждан';&#13;&#10;&#13;&#10;//  messageBox('&#34;'+s+'&#34;');&#13;&#10;                                         &#13;&#10;  n:=Pos('ORDER BY', Query1.SQL.text);&#13;&#10;&#13;&#10;  if cbRaion.Checked and not cbMy.Checked &#13;&#10;    then s:='SOAT,'&#13;&#10;    else s:='';                             &#13;&#10;  if cbSortNomer.Checked then begin&#13;&#10;    Query1.SQL.text := Copy(Query1.SQL.text,1,n-1)+' ORDER BY '+s+'NOMER';&#13;&#10;  end else begin&#13;&#10;    Query1.SQL.text := Copy(Query1.SQL.text,1,n-1)+' ORDER BY '+s+'FAMILIA';&#13;&#10;  end;&#13;&#10;    &#13;&#10;///  sYear  := Trim(edGod.KeyIndex);&#13;&#10;  sMonth := Trim(edMonth.KeyIndex);&#13;&#10;  sYear  := edGod.Text; // Trim(edGod.Items[edGod.ItemIndex]);    &#13;&#10;  lErr:=false;&#13;&#10;  if sYear='' then begin&#13;&#10;///    messagebox(' Не выбран год. ','Ошибка');&#13;&#10;    showMessage(' Не выбран год. ');&#13;&#10;    sYear:='1500';&#13;&#10;    lErr:=true;&#13;&#10;  end;&#13;&#10;  if sMonth='' then begin&#13;&#10;///    messagebox(' Не выбран месяц. ','Ошибка');&#13;&#10;    ShowMessage(' Не выбран месяц. ');&#13;&#10;    sMonth:='1';&#13;&#10;    lErr:=true;&#13;&#10;  end;&#13;&#10;//  sYear  := edGod.KeyItems[edGod.ItemIndex];&#13;&#10;//  sMonth := e