       &#13;&#10;  end else begin&#13;&#10;    cbRaion.Enabled:=true;&#13;&#10;    if RnGorod.Visible then  begin                                                         &#13;&#10;      RnGorod.Enabled:=true;&#13;&#10;      lbRn.Enabled:=true;&#13;&#10;    end;              &#13;&#10;  end;            &#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if cbRaion.Checked then begin&#13;&#10;    sRovd:=VarToStr(SeekValueSpr('SprRovd', 'SOATO', &#60;Query1.&#34;SOAT&#34;&#62;+'000', 'FNAME'));&#13;&#10;    if sRovd&#60;&#62;'' then begin&#13;&#10;      s:=VarToStr(SeekValueSpr('SprRovd', 'SOATO', &#60;Query1.&#34;SOAT&#34;&#62;+'000', 'ADRES'));&#13;&#10;      if s&#60;&#62;''&#13;&#10;        then sRovd:=sRovd+chr(13)+chr(10)+s;                                                                                                    &#13;&#10;    end;                  &#13;&#10;  end else begin                                  &#13;&#10;    sRovd:=Parameters('ROVD');&#13;&#10;  end;              &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" PropData="044C65667403A80203546F7003000108446174617365747301010C2600000020446174615365743D225175657279312220446174615365744E616D653D22517565727931220000095661726961626C65730100055374796C650100">
