34;WHO_SVID&#34;&#62;='Ж' then sWho:='Ж';&#13;&#10;    end else begin&#13;&#10;      if (&#60;CurDok.&#34;ON_SVID_NOMER&#34;&#62; ='')  and (&#60;CurDok.&#34;ONA_SVID_NOMER&#34;&#62; &#60;&#62; '') then sWho:='Ж';                                                                                       &#13;&#10;      if (&#60;CurDok.&#34;ONA_SVID_NOMER&#34;&#62; ='') and (&#60;CurDok.&#34;ON_SVID_NOMER&#34;&#62; &#60;&#62; '') then sWho:='М';                                                                                       &#13;&#10;    end;                    &#13;&#10;  end else begin&#13;&#10;    sWho:=VarToStr(v);&#13;&#10;  end;              &#13;&#10;  if (sWho='М') or (sWho='Ж') then begin&#13;&#10;    DialogPage1.Visible:=false;                                      &#13;&#10;  end else begin&#13;&#10;    sWho:='';                                 &#13;&#10;    DialogPage1.Visible:=true;                                      &#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogPage1OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  Button1.Enabled := false;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Page2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Page2, Engine do&#13;&#10;  begin&#13;&#10;  if sWho='' then begin&#13;&#10;    if rbM.Checked then sWho:='М' else sWho:='Ж';      &#13;&#10;  end;            &#13;&#10;    &#13;&#10;  Ruk_B := &#60;CurDok.&#34;Rukov_B&#34;&#62;;&#13;&#10;  Ruk := &#60;CurDok.&#34;Rukov&#34;&#62;;&#13;&#10;  if OnlySvid() then begin&#13;&#10;    dSvid := &#60;CurDok.&#34;DATESV&#34;&#62;;&#13;&#10;  end else begin&#13;&#10;    if &#60;CurDok.&#34;POVTOR&#34;&#62; then begin&#13;&#10;      LastDataSet('fmRastBrak.SvidPovtor');&#13;&#10;      dSvid := &#60;SvidPovtor.&#34;SVID_DATE&#34;&#62;;&#13;&#10;      Ruk_B := Parameters('РУК_ЗАГС_Б');&#13;&#10;      Ruk := Parameters('РУК_ЗАГС');&#13;&#10;    end else begin&#13;&#10;      if sWho='М' then begin&#13;&#10;        dSvid := &#60;CurDok.&#34;ON_SVID_DATE&#34;&#62;;&#13;&#10;      end else begin&#13;&#10;        dSvid := &#60;CurDok.&#34;ONA_SVID_DATE&#34;&#62;;&#13;&#10;      end;&#13;&#10;    end;&#13;&#10;  end;            &#13;&#10;  strDaySvid := Str(DayOf(dSvid));&#13;&#10;  strMonthSvidB := GetMonthPropis(dSvid,'B');&#13;&#10;  strMonthSvidR := GetMonthPropis(dSvid,'R');&#13;&#10;  strYearSvid := Copy(str(YearOf(dSvid)),3,2);&#13;&#10;&#13;&#10;  if sWho='М' then begin&#13;&#10;    setTypePrintSvidRast(1);  // установим для функции контроля выдаваемых свидетельств, что выдаем мужчине&#13;&#10;    s := CEE(&#60;CurDok.&#34;ON_Familia_B_PD&#34;&#62;) + ' ' +&#13;&#10;         CEE(&#60;CurDok.&#34;ON_Name_B_PD&#34;&#62;) + ' ' +&#13;&#10;         CEE(&#60;CurDok.&#34;ON_Otch_B_PD&#34;&#62;);&#13;&#10;    strKomuB:=Trim(s);&#13;&#10;    InitMemoLine(strKomuB, '15,100', true);&#13;&#10;    strKomuB1 :=Trim(GetMemoLine(1));&#13;&#10;    strKomuB2 :=Trim(GetMemoLine(2));&#13;&#10;&#13;&#10;    s := CEE(&#60;CurDok.&#34;ON_Familia_PD&#34;&#62;) + ' ' +&#13;&#10;         CEE(&#60;CurDok.&#34;ON_Name_PD&#34;&#62;) + ' ' +&#13;&#10;         CEE(&#60;CurDok.&#34;ON_Otch_PD&#34;&#62;);&#13;&#10;    strKomuR:=Trim(s);&#13;&#10;    InitMemoLine(strKomuR, '15,100', true);&#13;&#10;    strKomuR1 :=Trim(GetMemoLine(1));&#13;&#10;    strKomuR2 :=Trim(GetMemoLine(2));&#13;&#10;//    MessageBox('MMMMMMMMMM');&#13;&#10;  end;&#13;&#10;  if sWho='Ж' then begin&#13;&#10;    setTypePrintSvidRast(2);  // установим для функции контроля выдаваемых свидетельств, что выдаем женщине&#13;&#10;    s := CEE(&#60;CurDok.&#34;ONA_Familia_B_PD&#34;&#62;) + ' ' +&#13;&#10;         CEE(&#60;CurDok.&#34;ONA_Name_B_PD&#34;&#62;) + ' ' +&#13;&#10;         CEE(&#60;CurDok.&#34;ONA_Otch_B_PD&#34;&#62;);&#13;&#10;    strKomuB:=Trim(s);&#13;&#10;    InitMemoLine(strKomuB, '15,100', true);&#13;&#10;    strKomuB1 :=Trim(GetMemoLine(1));&#13;&#10;    strKomuB2 :=Trim(GetMemoLine(2));&#13;&#10;&#13;&#10;    s := CEE(&#60;CurDok.&#34;ONA_Familia_PD&#34;&#62;) + ' ' +&#13;&#10;         CEE(&#60;CurDok.&#34;ONA_Name_PD&#34;&#62;) + ' ' +&#13;&#10;         CEE(&#60;CurDok.&#34;ONA_Otch_PD&#34;&#62;);&#13;&#10;    strKomuR:=Trim(s);&#13;&#10;    InitMemoLine(strKomuR, '15,100', true);&#13;&#10;    strKomuR1 :=Trim(GetMemoLine(1));&#13;&#10;    strKomuR2 :=Trim(GetMemoLine(2));&#13;&#10; //    MessageBox('GGGGGGGGGGG');&#13;&#10;  end;&#13;&#10;&#13;&#10;  strZagsR := Trim(SprNameOrg(&#60;CurDok.&#34;ID_ZAGS&#34;&#62;,'',''));&#13;&#10;  InitMemoLine(strZagsR, '25,100', true);&#13;&#10;  strZagsR1 := Trim(GetMemoLine(1));&#13;&#10;  strZagsR2 := Trim(GetMemoLine(2));&#13;&#10;&#13;&#10;  strZagsB:=Trim(SprNameOrg(&#60;CurDok.&#34;ID_ZAGS&#34;&#62;,'B',''));&#13;&#10;  InitMemoLine(strZagsB, '25,100', true);&#13;&#10;  strZagsB1 := Trim(GetMemoLine(1));&#13;&#10;  strZagsB2 := Trim(GetMemoLine(2));&#13;&#10;&#13;&#10;  mPovtor.Visible := &#60;CurDok.&#34;POVTOR&#34;&#62; and &#60;CurDok.&#34;PRINT_POVTOR&#34;&#62;;&#13;&#10;  mPovtorB.Visible := &#60;CurDok.&#34;POVTOR&#34;&#62; and &#60;CurDok.&#34;PRINT_POVTOR&#34;&#62;;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;//  MessageBox(Parameters(' main_select'),'');&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure rbMOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with rbM, Engine do&#13;&#10;  begin&#13;&#10;  Button1.Enabled := true;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure rbGOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with rbG, Engine do&#13;&#10;  begin&#13;&#10;  Button1.Enabled := true;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" OnStartReport="frxReportOnStartReport" OnReportPrint="frxReportOnReportPrint" PropData="">
