<TfrxReport Version="4.12" DotMatrixReport="False" IniFile="E:\Projects\Selsovet7\Work\FastReports.ini" OldStyleProgress="True" PreviewOptions.Buttons="4095" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="38147.5166549074" ReportOptions.Description.Text="{ONPRINTREPORT}&#13;&#10;{NAME=SVIDBRAK}" ReportOptions.Name="Свидетельсво о браке" ReportOptions.LastChange="43790.5002203819" ReportOptions.VersionRelease="99999" ScriptLanguage="PascalScript" ScriptText.Text="Var&#13;&#10;  dDateBrak,dSvid:Date;&#13;&#10;  lUpper:Boolean;&#13;&#10;  sKrat,s,s1,s2,s3,s4,strMestoRogdM_R1,strMestoRogdM_R2,strMestoRogdG_R1,strMestoRogdG_R2:String;&#13;&#10;  strMestoRogdM_B1,strMestoRogdM_B2,strMestoRogdG_B1,strMestoRogdG_B2,strZagsR1,strZagsR2,strZagsB1,strZagsB2:String;&#13;&#10;  Ruk_B,Ruk,strPropDateR1,strPropDateR2,strPropDateB1,strPropDateB2,strPropDateR,strPropDateB:String;&#13;&#10;  strZagsR,strZagsB:String;                                                    &#13;&#10;  ds:TfrxDataSet;&#13;&#10;  i,n,nl,nLen:Integer;                            &#13;&#10;  lTwo:Boolean;                        &#13;&#10;  aMemo : array [1..16] of TfrxMemoView;  &#13;&#10;    &#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Page1, Engine do&#13;&#10;  begin&#13;&#10;&#13;&#10;  s:=Parameters('SV_BR_2CHAR');&#13;&#10;  if (s='') or (s='1')&#13;&#10;    then lTwo:=true&#13;&#10;    else lTwo:=false;                                         &#13;&#10;  &#13;&#10;  if DateIsEmpty(&#60;CurDok.&#34;DATEB&#34;&#62;) then begin&#13;&#10;    dDateBrak := &#60;CurDok.&#34;DATEZ&#34;&#62;;&#13;&#10;  end else begin&#13;&#10;    dDateBrak := &#60;CurDok.&#34;DATEB&#34;&#62;;&#13;&#10;  end;&#13;&#10;  lUpper:=false;&#13;&#10;  sKrat:='';&#13;&#10;  if ParamAsBoolean('SVID_KRAT') then begin  // печать в свидетельстве краткого назавания области района&#13;&#10;    sKrat:='&#60;.&#62;';&#13;&#10;  end;&#13;&#10;&#13;&#10;//  MessageBox(Str(TypeRS(5)),'');&#13;&#10;&#13;&#10;  //--- место рождения ОН по-русски&#13;&#10;  s1:=GetAdresAkt('fmZapisBrak.Dokument', sKrat+'ON_GOSUD,FName;ON_OBL,ON_B_OBL;ON_RAION;ON_GOROD,ON_B_GOROD;',1);&#13;&#10;  if AdresIsUpper() then lUpper:=true;&#13;&#10;//  if s1='' then s1:='сведения не представлены';&#13;&#10;  //--- место рождения ОНА по-русски&#13;&#10;  s2:=GetAdresAkt('fmZapisBrak.Dokument', sKrat+'ONA_GOSUD,FName;ONA_OBL,ONA_B_OBL;ONA_RAION;ONA_GOROD,ONA_B_GOROD;',1);&#13;&#10;  if AdresIsUpper() then lUpper:=true;&#13;&#10;//  if s2='' then s2:='сведения не представлены';&#13;&#10;  //--- место рождения ОН по-белорусски&#13;&#10;  s3:=GetAdresAkt('fmZapisBrak.Dokument', '&#60;B&#62;'+sKrat+'ON_GOSUD,FName_B;ON_OBL_B,ON_B_OBL;ON_RAION_B;ON_GOROD_B,ON_B_GOROD;',1);&#13;&#10;  s3:=REPLACE(s3, 'вобласть', 'вобласць');&#13;&#10;  s3:=REPLACE(s3, 'ВОБЛАСТЬ', 'ВОБЛАСЦЬ');&#13;&#10;  s3:=REPLACE(s3, 'д. ', 'в. ');&#13;&#10;  s3:=REPLACE(s3, 'Д. ', 'В. ');&#13;&#10;&#13;&#10;  if AdresIsUpper() then lUpper:=true;&#13;&#10;//  if s3='' then s1:='звесткі не прадстаўлены';&#13;&#10;  //--- место рождения ОНА по-белорусски&#13;&#10;  s4:=GetAdresAkt('fmZapisBrak.Dokument', '&#60;B&#62;'+sKrat+'ONA_GOSUD,FName_B;ONA_OBL_B,ONA_B_OBL;ONA_RAION_B;ONA_GOROD_B,ONA_B_GOROD;',1);&#13;&#10;  s4:=REPLACE(s4, 'вобласть', 'вобласць');&#13;&#10;  s4:=REPLACE(s4, 'ВОБЛАСТЬ', 'ВОБЛАСЦЬ');&#13;&#10;  s4:=REPLACE(s4, 'д. ', 'в. ');&#13;&#10;  s4:=REPLACE(s4, 'Д. ', 'В. ');&#13;&#10;&#13;&#10;  if AdresIsUpper() then lUpper:=true;&#13;&#10;//  if s4='' then s4:='звесткі не прадстаўлены';&#13;&#10;&#13;&#10;  if lUpper then begin&#13;&#10;    s1:=UpperCase(s1);&#13;&#10;    s2:=UpperCase(s2);&#13;&#10;    s3:=UpperCase(s3);&#13;&#10;    s4:=UpperCase(s4);&#13;&#10;  end;&#13;&#10;// !!!!!!!!!!!!!!!!!!!!!!!!!!&#13;&#10;//  if TypeRS(5)=2&#13;&#10;//    then si:='Q32,75'&#13;&#10;//    else si:='Q35,75';&#13;&#10;&#13;&#10;  //si:='Q32,75';&#13;&#10;&#13;&#10;  if ((TypeRS(5)=2) or lUpper) and ((Length(Trim(s1))&#62;65) or (Length(Trim(s2))&#62;65)) then begin  // перчать свид. в верхнем регистре&#13;&#10;    n:=9;&#13;&#10;    nl:=13;&#13;&#10;  end else begin&#13;&#10;    n:=10;&#13;&#10;    nl:=10;&#13;&#10;  end;&#13;&#10;  {mMestoON_B.Font.Size:=n;              &#13;&#10;  mMestoON_B.LineSpacing:=nl;                                                  &#13;&#10;  mMestoON_R.Font.Size:=n;&#13;&#10;  mMestoON_R.LineSpacing:=nl;                                                  &#13;&#10;  mMestoONA_B.Font.Size:=n;&#13;&#10;  mMestoONA_B.LineSpacing:=nl;                                                  &#13;&#10;  mMestoONA_R.Font.Size:=n;&#13;&#10;  mMestoONA_R.LineSpacing:=nl; }  &#13;&#10;&#13;&#10;  nLen:=16;  &#13;&#10;  aMemo[1]:=mMestoON_B1;              &#13;&#10;  aMemo[2]:=mMestoON_B1;&#13;&#10;  aMemo[3]:=mMestoON_B2;              &#13;&#10;  aMemo[4]:=mMestoON_B2;&#13;&#10;      &#13;&#10;  aMemo[5]:=mMestoON_R1;&#13;&#10;  aMemo[6]:=mMestoON_R1;&#13;&#10;  aMemo[7]:=mMestoON_R2;&#13;&#10;  aMemo[8]:=mMestoON_R2;&#13;&#10;      &#13;&#10;  aMemo[9]:=mMestoONA_B1;&#13;&#10;  aMemo[10]:=mMestoONA_B1;&#13;&#10;  aMemo[11]:=mMestoONA_B2;&#13;&#10;  aMemo[12]:=mMestoONA_B2;&#13;&#10;      &#13;&#10;  aMemo[13]:=mMestoONA_R1;&#13;&#10;  aMemo[14]:=mMestoONA_R1;&#13;&#10;  aMemo[15]:=mMestoONA_R2;&#13;&#10;  aMemo[16]:=mMestoONA_R2;&#13;&#10;    &#13;&#10;  for i:=1 to nLen do begin &#13;&#10;    aMemo[i].Font.Size:=n;  &#13;&#10;   // aMemo[i].LineSpacing:=nl;    &#13;&#10;  end;&#13;&#10;    &#13;&#10;// !!!!!!!!!!!!!!!!!!!!!!!!!&#13;&#10;{    &#13;&#10;  InitMemoLine(s1, si, true);&#13;&#10;  strMestoRogdM_R1 := Trim(GetMemoLine(1));&#13;&#10;  strMestoRogdM_R2 := Trim(GetMemoLine(2));&#13;&#10;&#13;&#10;  InitMemoLine(s2, si, true);&#13;&#10;  strMestoRogdG_R1 := Trim(GetMemoLine(1));&#13;&#10;  strMestoRogdG_R2 := Trim(GetMemoLine(2));&#13;&#10;&#13;&#10;  InitMemoLine(s3, si, true);&#13;&#10;  strMestoRogdM_B1 := Trim(GetMemoLine(1));&#13;&#10;  strMestoRogdM_B2 := Trim(GetMemoLine(2));&#13;&#10;&#13;&#10;  InitMemoLine(s4, si, true);&#13;&#10;  strMestoRogdG_B1 := Trim(GetMemoLine(1));&#13;&#10;  strMestoRogdG_B2 := Trim(GetMemoLine(2));&#13;&#10; }  &#13;&#10;&#13;&#10;  strZagsR:=SprNameOrg(&#60;CurDok.&#34;ID_ZAGS&#34;&#62;,'','');                        &#13;&#10;  InitMemoLine(strZagsR, '24,100', true);&#13;&#10;  strZagsR1 := Trim(GetMemoLine(1));&#13;&#10;  strZagsR2 := Trim(GetMemoLine(2));&#13;&#10;  strZagsB:=SprNameOrg(&#60;CurDok.&#34;ID_ZAGS&#34;&#62;,'B','');                        &#13;&#10;  InitMemoLine(strZagsB, '24,100', true);&#13;&#10;  strZagsB1 := Trim(GetMemoLine(1));&#13;&#10;  strZagsB2 := Trim(GetMemoLine(2));&#13;&#10;&#13;&#10;  if ParamAsBoolean('DATE_SV_2STR') then begin&#13;&#10;    strPropDateR1 := GetDaysPropis(dDateBrak,'R')+' '+GetMonthPropis(dDateBrak,'R')+ ' ';&#13;&#10;    strPropDateR2 := GetYearPropis(dDateBrak,'R')+' года';&#13;&#10;    strPropDateB1 := GetDaysPropis(dDateBrak,'B')+' '+GetMonthPropis(dDateBrak,'B');&#13;&#10;    strPropDateB2 := GetYearPropis(dDateBrak,'B')+' года';&#13;&#10;  end else begin&#13;&#10;    strPropDateR := GetDaysPropis(dDateBrak,'R')+' '+&#13;&#10;                    GetMonthPropis(dDateBrak,'R')+ ' '+&#13;&#10;                    GetYearPropis(dDateBrak,'R')+' года';&#13;&#10;    strPropDateB := GetDaysPropis(dDateBrak,'B')+' '+&#13;&#10;                    GetMonthPropis(dDateBrak,'B')+ ' '+&#13;&#10;                    GetYearPropis(dDateBrak,'B')+' года';&#13;&#10;    InitMemoLine(strPropDateR, '45,50', true);&#13;&#10;    strPropDateR1 := Trim(GetMemoLine(1));&#13;&#10;    strPropDateR2 := Trim(GetMemoLine(2));&#13;&#10;    InitMemoLine(strPropDateB, '45,50', true);&#13;&#10;    strPropDateB1 := Trim(GetMemoLine(1));&#13;&#10;    strPropDateB2 := Trim(GetMemoLine(2));&#13;&#10;  end;&#13;&#10;  mPovtor.Visible := &#60;CurDok.&#34;POVTOR&#34;&#62; and &#60;CurDok.&#34;PRINT_POVTOR&#34;&#62;;&#13;&#10;  mPovtorB.Visible := &#60;CurDok.&#34;POVTOR&#34;&#62; and &#60;CurDok.&#34;PRINT_POVTOR&#34;&#62;;&#13;&#10;&#13;&#10;  Ruk_B := &#60;CurDok.&#34;Rukov_B&#34;&#62;;&#13;&#10;  Ruk := &#60;CurDok.&#34;Rukov&#34;&#62;;&#13;&#10;  if &#60;CurDok.&#34;POVTOR&#34;&#62; then begin&#13;&#10;    if OnlySvid()  then begin&#13;&#10;      dSvid := &#60;CurDok.&#34;DateSV&#34;&#62;;&#13;&#10;    end else begin&#13;&#10;//      ds:=Report.GetDataSet('SvidPovtor');&#13;&#10;//      ds.Last;&#13;&#10;      LastDataSet('fmZapisBrak.SvidPovtor');&#13;&#10;      dSvid := &#60;SvidPovtor.&#34;SVID_DATE&#34;&#62;;&#13;&#10;      Ruk_B := Parameters('РУК_ЗАГС_Б');&#13;&#10;      Ruk := Parameters('РУК_ЗАГС');&#13;&#10;    end;&#13;&#10;  end else begin&#13;&#10;    dSvid := &#60;CurDok.&#34;DateSV&#34;&#62;;&#13;&#10;  end;&#13;&#10;      &#13;&#10;//  MessageBox(Parameters(' main_select'),'');&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Memo1, Engine do&#13;&#10;  begin&#13;&#10;  if &#60;CurDok.&#34;ON_ONLYGOD&#34;&#62;&#13;&#10;    then s := '-------------------------------------'&#13;&#10;    else s := GetMonthPropis(&#60;CurDok.&#34;ON_DateR&#34;&#62;,'R');&#13;&#10;    if &#60;CurDok.&#34;ON_VOZR&#34;&#62;&#62;0 then s := 'исполнилось '+GetVozrast(&#60;CurDok.&#34;ON_VOZR&#34;&#62;);&#13;&#10;    text:=RS(s,5);&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Memo2, Engine do begin&#13;&#10;  if &#60;CurDok.&#34;ON_ONLYGOD&#34;&#62;&#13;&#10;    then text := '------'&#13;&#10;    else text := FormatDateTime('dd',&#60;CurDok.&#34;ON_DATER&#34;&#62;) ;&#13;&#10;    if &#60;CurDok.&#34;ON_VOZR&#34;&#62; &#62;0 then text := '';&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo3OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Memo3, Engine do  begin&#13;&#10;   text :=Str(YearOf(&#60;CurDok.&#34;ON_DateR&#34;&#62;));&#13;&#10;   if (Copy(text,1,2)='19') and lTwo then text:=Copy(text,3,2);&#13;&#10;   if &#60;CurDok.&#34;ON_VOZR&#34;&#62;&#62;0 then text := '';&#13;&#10;  end;  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo8OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Memo8, Engine do&#13;&#10;  begin&#13;&#10;  if &#60;CurDok.&#34;ON_ONLYGOD&#34;&#62;&#13;&#10;    then s := '-------------------------------------'&#13;&#10;    else s := GetMonthPropis(&#60;CurDok.&#34;ON_DateR&#34;&#62;,'B');&#13;&#10;    if &#60;CurDok.&#34;ON_VOZR&#34;&#62; &#62;0 then s := 'споўнілася '+VozrastBel(&#60;CurDok.&#34;ON_VOZR&#34;&#62;,false);&#13;&#10;    text:=RS(s,5);&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo9OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Memo9, Engine do&#13;&#10;  begin&#13;&#10;  if &#60;CurDok.&#34;ON_ONLYGOD&#34;&#62;&#13;&#10;    then text := '------'&#13;&#10;    else text := FormatDateTime('dd',&#60;CurDok.&#34;ON_DATER&#34;&#62;) ;&#13;&#10;    if &#60;CurDok.&#34;ON_VOZR&#34;&#62; &#62;0 then text := '';&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo10OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Memo10, Engine do&#13;&#10;  begin&#13;&#10;   text :=str(YearOf(&#60;CurDok.&#34;ON_DateR&#34;&#62;));&#13;&#10;   if (Copy(text,1,2)='19') and lTwo then text:=Copy(text,3,2);&#13;&#10;   if &#60;CurDok.&#34;ON_VOZR&#34;&#62; &#62;0 then text := '';&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo15OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Memo15, Engine do&#13;&#10;  begin&#13;&#10;  if &#60;CurDok.&#34;ONA_ONLYGOD&#34;&#62;&#13;&#10;    then s := '-------------------------------------'&#13;&#10;    else s := GetMonthPropis(&#60;CurDok.&#34;ONA_DateR&#34;&#62;,'R');&#13;&#10;    if &#60;CurDok.&#34;ONA_VOZR&#34;&#62; &#62;0 then s := 'исполнилось '+GetVozrast(&#60;CurDok.&#34;ONA_VOZR&#34;&#62;);&#13;&#10;    text:=RS(s,5);&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo16OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Memo16, Engine do&#13;&#10;  begin&#13;&#10;  if &#60;CurDok.&#34;ONA_ONLYGOD&#34;&#62;&#13;&#10;    then text := '------'&#13;&#10;    else text := FormatDateTime('dd',&#60;CurDok.&#34;ONA_DATER&#34;&#62;) ;&#13;&#10;    if &#60;CurDok.&#34;ONA_VOZR&#34;&#62; &#62;0 then text := '';&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo17OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Memo17, Engine do begin&#13;&#10;   text := Str(YearOf(&#60;CurDok.&#34;ONA_DateR&#34;&#62;));&#13;&#10;   if (Copy(text,1,2)='19') and lTwo then text:=Copy(text,3,2);&#13;&#10;   if &#60;CurDok.&#34;ONA_VOZR&#34;&#62; &#62;0 then text := '';&#13;&#10;  end;    &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo18OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Memo18, Engine do&#13;&#10;  begin&#13;&#10;  if &#60;CurDok.&#34;ONA_ONLYGOD&#34;&#62;&#13;&#10;    then s := '-------------------------------------'&#13;&#10;    else s := GetMonthPropis(&#60;CurDok.&#34;ONA_DateR&#34;&#62;,'B');&#13;&#10;    if &#60;CurDok.&#34;ONA_VOZR&#34;&#62; &#62;0 then s := 'споўнілася '+VozrastBel(&#60;CurDok.&#34;ONA_VOZR&#34;&#62;,false);&#13;&#10;    text:=RS(s,5);&#13;&#10;  end;  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo19OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Memo19, Engine do&#13;&#10;  begin&#13;&#10;  if &#60;CurDok.&#34;ONA_ONLYGOD&#34;&#62;&#13;&#10;    then text := '------'&#13;&#10;    else text := FormatDateTime('dd',&#60;CurDok.&#34;ONA_DATER&#34;&#62;) ;&#13;&#10;    if &#60;CurDok.&#34;ONA_VOZR&#34;&#62; &#62;0 then text := '';&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo20OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Memo20, Engine do&#13;&#10;  begin&#13;&#10;   text :=Str(YearOf(&#60;CurDok.&#34;ONA_DateR&#34;&#62;));&#13;&#10;   if (Copy(text,1,2)='19') and lTwo then text:=Copy(text,3,2);&#13;&#10;   if &#60;CurDok.&#34;ONA_VOZR&#34;&#62; &#62;0 then text := '';&#13;&#10;end&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure frxReportOnRunDialogs(var Result: Boolean);&#13;&#10;begin&#13;&#10;  with Page1 do begin          &#13;&#10;    Memo42.WordBreak:=ParamAsBoolean('SV_WORD_ORG');&#13;&#10;    Memo44.WordBreak:=ParamAsBoolean('SV_WORD_ORG');&#13;&#10;//    if Memo29.WordBreak then showmessage('Yes') else showmessage('No');&#13;&#10;    n:=ParamAsInteger('SV_FONT_ORG');&#13;&#10;    if (n&#62;=8) and (n&#60;=10) then begin&#13;&#10;      Memo42.Font.Size:=n;&#13;&#10;      Memo44.Font.Size:=n;&#13;&#10;    end;&#13;&#10;  end;            &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" OnRunDialogs="frxReportOnRunDialogs" OnReportPrint="frxReportOnReportPrint" PropData="">
